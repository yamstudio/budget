{
  "name": "budget",
  "version": "0.1.0",
  "description": "UI for Budgeting App",
  "homepage": "https://github.com/yamstudio/budget",
  "license": "MIT",
  "author": "Yujun Qin <yamstudio@users.noreply.github.com>",
  "private": true,
  "dependencies": {
    "ag-grid-react": "^32.3.4",
    "antd": "^5.24.6",
    "axios": "^1.8.4",
    "chart.js": "^4.4.8",
    "date-fns": "^4.1.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "swagger-typescript-api": "^13.0.28",
    "typescript": "^5.8.2",
    "vite": "^5.4.17"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src",
    "prettier": "prettier -w .",
    "serve": "vite preview",
    "genclient": "swagger-typescript-api generate -p swagger/swagger.json -o src/gensrc --axios"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "ignorePatterns": [
      "src/gensrc/**/*"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": [
          "src/**/*.{ts,tsx}"
        ]
      }
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 140
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
